<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">

<hibernate-mapping> 
	<class name="Entity.Section" table="sections">
		<id name="sectionId" column="section_id">
			<generator class="increment"></generator>
		</id>

		<property name="sectionCode" column="section_code"></property>
		<property name="sectionName" column="section_name"></property>
		<property name="companyId" column="company_id"></property>

	</class>

</hibernate-mapping>